<div class="ibox float-e-margins">
  <div class="ibox-title">
    <h5 class="show-form-link">
      {% if model.isNew %}
        New Attribute Set
      {% else %}
        Edit Attribute Set #{{ model.id }}
        <small>{{ model.title }}</small>
      {% endif %}
    </h5>
  </div>
  <div class="ibox-content">
    <form class="form-horizontal">
      <div class="form-group">
        <label class="col-sm-2 control-label">Title</label>

        <div class="col-sm-10">
          <input type="text" name="title" class="form-control">
        </div>
      </div>
      <div class="hr-line-dashed"></div>

      <div class="model-attributes">
        <span style="text-align: center">... загрузка ...</span>
        <!-- attributes -->
      </div>
      <div class="for-draggable">перетащить сюда атрибут</div>
      <div class="hr-line-dashed"></div>
      <div class="error-container"><!-- --></div>
      <div class="form-group">
        <div class="col-sm-4 col-sm-offset-2">
          <button class="btn btn-primary save">Save changes</button>
          <button class="btn btn-white hide-form">Cancel</button>
        </div>
      </div>
    </form>
  </div>
</div>
